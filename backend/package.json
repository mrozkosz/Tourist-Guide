    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-validator": "^6.5.0",
    "express-ws": "^4.0.0",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "gm": "^1.23.1",
    "http-status-codes": "^1.4.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.27",
    "moment": "^2.26.0",
    "mongoose": "^5.9.16",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "node-cron": "^2.0.3",
    "node-dependency-injection": "^2.6.3",
    "node-fetch": "^2.6.1",
    "node-image-resizer": "^1.0.0",
    "nodemailer": "^6.4.8",
    "passport-facebook-token": "^4.0.0",
    "path": "^0.12.7",
    "sequelize": "^4.44.4",
